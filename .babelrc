{
  "presets": [
    [
      "@babel/preset-env",
      {
        "shippedProposals": true,
        "targets": { "node": "current" }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@api": "./src/api",
          "@config": "./src/config",
          "@loaders": "./src/loaders",
          "@models": "./src/models",
          "@services": "./src/services"
        }
      }
    ]
  ]
}
